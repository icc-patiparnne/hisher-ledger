{
  "name": "console-v3",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "copy-traces-file": "cp ./../../packages/logger/serverTracer.mjs ./serverTracer.mjs",
    "dev": "pnpm copy-traces-file && NODE_OPTIONS='--import ./serverTracer.mjs' react-router dev",
    "lint": "eslint app",
    "start:prod": "pnpm copy-traces-file && OTEL_SERVICE_NAME=console-v3 NODE_OPTIONS='--import ./serverTracer.mjs' react-router-serve ./build/server/index.js",
    "start:prod:local": "export $(cat .env | xargs) && pnpm build && pnpm start:prod",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@platform/apps": "workspace:*",
    "@platform/http": "workspace:*",
    "@platform/logger": "workspace:*",
    "@platform/remix": "workspace:*",
    "@platform/sdks": "workspace:*",
    "@platform/utils": "workspace:*",
    "@react-router/node": "^7.6.2",
    "@react-router/remix-routes-option-adapter": "^7.6.2",
    "@react-router/serve": "^7.6.2",
    "@xyflow/react": "^12.7.0",
    "graphviz-react": "^1.2.5",
    "isbot": "^4.4.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "^7.6.2",
    "recharts": "^2.15.4",
    "tiny-invariant": "^1.3.3",
    "ts-graphviz": "^2.1.6"
  },
  "devDependencies": {
    "@platform/eslint-config": "workspace:*",
    "@platform/ts-config": "workspace:*",
    "@platform/ui": "workspace:*",
    "@react-router/dev": "^7.6.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "vite": "^6.3.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=22.15.0"
  }
}
